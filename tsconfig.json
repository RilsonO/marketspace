{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@dtos/*": ["src/dtos/*"],
      "@assets/*": ["./src/assets/*"],
      "@views/*": ["./src/views/*"],
      "@infra/*": ["./src/infra/*"],
      "@utils/*": ["./src/utils/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@view-models/*": ["./src/view-models/*"],
      "@routes/*": ["./src/routes/*"],
      "@mappers/*": ["./src/mappers/*"]
    }
  }
}
